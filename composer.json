{
    "name": "webappid/member",
    "description": "Core simple member management for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Dyan Galih",
            "email": "dyan.galih@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "WebAppId\\Member\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebAppId\\Member\\Tests\\": "tests/",
            "WebAppId\\User\\Tests\\": "vendor/webappid/laravel-user/tests"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^6.0|^7.0",
        "doctrine/dbal": "^2.5",
        "webpatser/laravel-uuid": "^3.0",
        "webappid/laravel-user": "^4.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0|^5.0",
        "webappid/lazycrud": "^1.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "WebAppId\\Member\\ServiceProvider"
            ]
        }
    }
}
