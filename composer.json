{
    "name": "webappid/member",
    "description": "Core simple member management for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Dyan Galih",
            "email": "dyan.galih@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "WebAppId\\Member\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebAppId\\Member\\Tests\\": "tests/",
            "WebAppId\\User\\Tests\\": "vendor/webappid/laravel-user/tests",
            "WebAppId\\Content\\Tests\\" : "vendor/webappid/content/tests"
        }
    },
    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "^7.0|^8.0",
        "doctrine/dbal": "^2.5",
        "webappid/laravel-user": "^4.0",
        "nesbot/carbon": "^2.0",
        "webappid/content": "^5.0",
        "webappid/lazy": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "WebAppId\\Member\\ServiceProvider"
            ]
        }
    }
}
